<%- include('../partials/header')  %>

<table>
    <tbody>
        <% covens.forEach(function(c) { %>
            <tr>
                <td><img class="covenAvatar" src="<%= c.image %>"></td>
                <td><a href="/covens/<%= c._id %>"><%= c.name %></a></td>
                <td><%= c.description%></td>
            </tr>
        <% });%>
    </tbody>
</table>
<br>
    
<form action="/covens/new" method="GET">
    <button>Found a new Coven</button>
</form>

<%- include('../partials/footer')  %>